
<h1>Reseñas sobre <%=@user_receive.name%></h1>

<ul>
    <% @reviews.each do |review| %> 
        <li>
            <%= "Feedback: #{review.feedback} "%>
        </li>
            <%= "Score: #{review.score} "%>
    <% end %>
</ul>

<%= link_to 'Nueva Reseña', new_user_review_path(@user) %>

<%= link_to 'Volver', user_path(@user) %>





