<h1>Edit Review</h1>
<%= form_with model: @review do |form| %>
<div>
    <%= form.label :feedback %>
    <%= form.text_field :feedback %>
    <% @review.errors.full_messages_for(:feedback).each do |message| %>
        <div><%= message %></div>
    <% end %>
</div>
<div>
    <%= form.label :score %>
    <%= form.number_field :score %>
    <% @review.errors.full_messages_for(:score).each do |message| %>
        <div><%= message %></div>
    <% end %>
</div>
<div>
    <%= form.submit %>
</div>
<% end %>